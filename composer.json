{
    "name": "dara/liteorm",
    "description": "A database management system",
    "keywords": [
        "Dara",
        "ORM",
        "database"
    ],
    "homepage": "https://github.com/andela-doladosu/liteorm",
    "license": "MIT",
    "authors": [
        {
            "name": "Dara Oladosu",
            "email": "dara.Oladosu@andela.com",
            "homepage": "http://github.com/andela-doladosu",
            "role": "Developer"
        }
    ],
    "minimum-stability" : "dev",
    "require": {
        "php" : ">=5.3.0",
        "vlucas/phpdotenv": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Dara\\": "src"
        },
        "classmap": ["src"]
    },
    "autoload-dev": {
        "psr-4": {
            "Dara\\Test\\": "tests"
        },
        "classmap": ["tests"]
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
